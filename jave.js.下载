// JavaScript Document

//change the background color of the paragrom
function changeBackgroundColor1()
{
	
	document.getElementById("paragraph1").setAttribute("style","background-color: lightyellow") ;
}

function changeBackgroundColor2()
{
	
	document.getElementById("paragraph1").setAttribute("style","background-color: ");	
}

